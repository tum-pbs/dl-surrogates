FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (VEL_X VEL_Y 0);

boundaryField
{
    inlet
    {
        type            freestream;
        freestreamValue uniform (VEL_X VEL_Y 0);
    }

    exit
    {
        type            freestream;
        freestreamValue uniform (VEL_X VEL_Y 0);
    }

    top
    {
        type            freestream;
        freestreamValue uniform (VEL_X VEL_Y 0);
    }

    bottom
    {
        type            freestream;
        freestreamValue uniform (VEL_X VEL_Y 0);
    }

    aerofoil
    {
        type            fixedValue;
	value		uniform (0 0 0);
    }

    front
    {
        type            empty;
    }

    back
    {
        type            empty;
    }
}

// ************************************************************************* //
